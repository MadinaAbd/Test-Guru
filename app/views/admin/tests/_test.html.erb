<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('.show'), admin_test_path(test), class: 'btn btn-outline-primary' %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test), class: 'btn btn-outline-primary' %></td>

  <td><%= link_to t('.delete'),
                class: 'btn btn-outline-primary',
                admin_test_path(test),
                method: :delete,
                data: { confirm: t('.delete_confirm') } %></td>
</tr>
